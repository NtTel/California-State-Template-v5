{% extends "./base.njk" %}
{% block content %}
<div id="main-content" class="main-content">
	<main class="main-primary" id="main">
		<div class="container p-b-lg p-t-md">
			<div class="row">
				<div class="col-md-3">
					<nav aria-labelledby="components-list">
						<div class="h4 m-t-0" id="components-list">Sidenav</div>
						<ul class="list-navigation">
						{% for item in side.sidenav %}
						<li {{ helpers.getLinkActiveState(item.url,
								page.url) | safe }}>
								<a href="{{ item.url }}">{{ item.text }}</a>
         {% if subnav in item %}
           {% for subitem in side.sidenav.subnav %}
           <ul>
            <li>
             <a href="{{ item.url }}">{{ item.text }}</a>
            </li>
           </ul>
           {% endfor %}
         {% endif %}
						</li>
						{% endfor %}
						</ul>
					</nav>
				</div>
				<div class="col-md-9">
					{{ content | safe }}
				</div>
			</div>
		</div>
	</main>
</div>
{% endblock %}